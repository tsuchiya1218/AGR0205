            <script type="text/javascript">
                img = new Array("../img/tatle1.jpg", "../img/tatle2.jpg", "../img/tatle3.jpg", "../img/tatle4.jpg"); //*1
                count = -1; //*2
                imgTimer();

                function imgTimer() {
                    //画像番号
                    count++; //*3
                    //画像の枚数確認
                    if (count == img.length) count = 0; //*4
                    //画像出力
                    document.sushi.src = img[count]; //*5
                    //次のタイマー呼びだし
                    setTimeout("imgTimer()", 1500); //*6
                }
            </script>
